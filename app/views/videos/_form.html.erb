<div class="row">

  <div class="col-md-6 col-md-offset-3">
  <%=form_for ([@user, @video]), :html => {:multipart => true} do |f|%>
    <%= f.label :title, "Add Title" %><br>
    <%= f.text_field :title, class: 'form-control'%><br>
    <%= f.label :data_content, "Add Video"%><br>
    <%= f.file_field :data_content, class: 'form-control' %><br>
    <%= f.submit "Submit", class: "btn btn-primary" %>
  <% end %>
  </div>
</div>
